# NBA Stats Tracker - Environment Variables
# Copy this file to .env and add your actual API keys

# NBA API Key (if using a real API service)
# REACT_APP_NBA_API_KEY=your_api_key_here

# Example: Ball Don't Lie API (free tier)
# REACT_APP_NBA_API_URL=https://api.balldontlie.io/v1

# Example: RapidAPI NBA Stats
# REACT_APP_RAPIDAPI_KEY=your_rapidapi_key_here
# REACT_APP_RAPIDAPI_HOST=api-nba-v1.p.rapidapi.com

# Development settings
REACT_APP_ENV=development
REACT_APP_DEBUG=true
